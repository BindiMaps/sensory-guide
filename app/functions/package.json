{
  "name": "functions",
  "main": "lib/index.js",
  "scripts": {
    "sync-schemas": "bash scripts/sync-schemas.sh",
    "prebuild": "npm run sync-schemas",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "predeploy": "npm run sync-schemas && npm run build",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "lint": "eslint src --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:pdf": "tsx scripts/test-pdf-extraction.ts"
  },
  "engines": {
    "node": "22"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "firebase-admin": "^13.4.0",
    "firebase-functions": "^6.3.2",
    "pdf-parse": "^2.4.5",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^20.17.0",
    "@types/pdf-parse": "^1.1.5",
    "tsx": "^4.21.0",
    "typescript": "^5.7.0",
    "vitest": "^4.0.18"
  },
  "private": true
}
